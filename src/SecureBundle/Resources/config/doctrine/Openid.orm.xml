<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SecureBundle\Entity\Openid" table="openid">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="uid" type="integer" column="uid" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="profileUrl" type="string" column="profile_url" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nickname" type="string" column="nickname" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="firstName" type="string" column="first_name" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="identity" type="string" column="identity" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="photoBig" type="string" column="photo_big" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="photo" type="string" column="photo" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="providerId" type="integer" column="provider_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="countryId" type="integer" column="country_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="userId" type="integer" column="user_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>

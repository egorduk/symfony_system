<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AuthBundle\Entity\UserOrder" table="user_order">
    <unique-constraints>
      <unique-constraint name="num" columns="num"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="num" type="integer" column="num" nullable="false">
      <options>
        <option name="unsigned">1</option>
      </options>
    </field>
    <field name="theme" type="string" column="theme" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="task" type="text" column="task" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="originality" type="integer" column="originality" nullable="false">
      <options>
        <option name="unsigned">1</option>
      </options>
    </field>
    <field name="countSheet" type="integer" column="count_sheet" nullable="false">
      <options>
        <option name="unsigned">1</option>
      </options>
    </field>
    <field name="dateCreate" type="datetime" column="date_create" nullable="false"/>
    <field name="dateExpire" type="datetime" column="date_expire" nullable="false"/>
    <field name="dateEdit" type="datetime" column="date_edit" nullable="false"/>
    <field name="dateComplete" type="datetime" column="date_complete" nullable="false"/>
    <field name="dateGuarantee" type="datetime" column="date_guarantee" nullable="false"/>
    <field name="dateCancel" type="datetime" column="date_cancel" nullable="false"/>
    <field name="dateConfirm" type="datetime" column="date_confirm" nullable="false"/>
    <field name="isShowAuthor" type="boolean" column="is_show_author" nullable="false"/>
    <field name="isShowClient" type="boolean" column="is_show_client" nullable="false"/>
    <field name="isDelay" type="boolean" column="is_delay" nullable="false"/>
    <field name="filesFolder" type="string" column="files_folder" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="clientComment" type="string" column="client_comment" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="clientDegree" type="integer" column="client_degree" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="subjectOrderId" type="integer" column="subject_order_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="typeOrderId" type="integer" column="type_order_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="userId" type="integer" column="user_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>

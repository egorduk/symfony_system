<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AuthBundle\Entity\UserInfo" table="user_info">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="skype" type="string" column="skype" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="mobilePhone" type="string" column="mobile_phone" length="17" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="staticPhone" type="string" column="static_phone" length="17" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="icq" type="string" column="icq" length="10" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="username" type="string" column="username" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="surname" type="string" column="surname" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lastname" type="string" column="lastname" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="countryId" type="integer" column="country_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
